<div class="container">
    <div class="row">
        <div class="col-md">
            <h1>Search Results</h1>
            <br>

            <% @wishlist_albums.each do |wishlist_album| %>

            <% if wishlist_album.artist.nil? %>
            <div>no</div>
            <% end %>

            <%= wishlist_album.artist %> <%= link_to wishlist_album.album_title, wishlist_album_path(wishlist_album) %><br>

            <% end %>

            <br>
            <%= link_to 'Back to wishlist', wishlist_albums_path, class: "btn btn-outline-primary" %>
        </div>
    </div>
</div>